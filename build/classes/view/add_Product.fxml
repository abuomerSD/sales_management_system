                  <Font name="Calibri" size="21.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtProductQuantity" labelFloat="true" nodeOrientation="RIGHT_TO_LEFT" prefHeight="43.0" prefWidth="660.0" promptText="العدد">
