<AnchorPane id="AnchorPane" prefHeight="439.0" prefWidth="555.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddProductController">
    <stylesheets>
        <URL value="@/resources/style.css" />
    </stylesheets>
   <children>
